@model Escon.SisctNET.Model.Company
       

<div class="row">
    <div class="form-group col-lg-4 col-md-4 col-xs-12">
        <label asp-for="PercentualPis" class="control-label"></label>
        <input class="form-control money" name="PercentualPis" value="@Model.PercentualPis" />
    </div>
    <div class="form-group col-lg-4 col-md-4 col-xs-12">
        <label asp-for="PercentualPisRF" class="control-label">PIS Rec. Financeira %</label>
        <input class="form-control money" name="PercentualPisRF" value="@Model.PercentualPisRF" />
    </div>
    <div class="form-group col-lg-4 col-md-4 col-xs-12">
        <label asp-for="PercentualPisCredito" class="control-label">PIS Crédito %</label>
        <input class="form-control money" name="PercentualPisCredito" value="@Model.PercentualPisCredito" />
    </div>
</div>

<div class="row">
    <div class="form-group col-lg-4 col-md-4 col-xs-12">
        <label asp-for="PercentualCofins" class="control-label"></label>
        <input class="form-control money" name="PercentualCofins" value="@Model.PercentualCofins" />
    </div>
    <div class="form-group col-lg-4 col-md-4 col-xs-12">
        <label asp-for="PercentualCofinsRF" class="control-label">COFINS Rec. Financeira %</label>
        <input class="form-control money" name="PercentualCofinsRF" value="@Model.PercentualCofinsRF" />
    </div>
    <div class="form-group col-lg-4 col-md-4 col-xs-12">
        <label asp-for="PercentualCofinsCredito" class="control-label">COFINS Crédito %</label>
        <input class="form-control money" name="PercentualCofinsCredito" value="@Model.PercentualCofinsCredito" />
    </div>
</div>

      
