@model Escon.SisctNET.Model.Company

<div class="col-lg-12">
    <form asp-action="Tax" method="post">
        <div class="row">
            <div class="form-group col-lg-2 col-md-6 col-xs-12">
                <label asp-for="IRPJ" class="control-label"></label>
                <input asp-for="IRPJ" class="form-control money" disabled/>
            </div>
            <div class="form-group col-lg-2 col-md-6 col-xs-12">
                <label asp-for="CSLL" class="control-label"></label>
                <input asp-for="CSLL" class="form-control money" disabled/>
            </div>
            <div class="form-group col-lg-1 col-md-6 col-xs-12">
                @Html.LabelFor(model => model.StatusCPRB, "Ativo", htmlAttributes: new { @class = "control-label col-md-12" })
                <div class="checkbox">
                    <label>
                        @Html.CheckBoxFor(model => model.StatusCPRB, new { @data_off = "Não", @data_on = "Sim", @data_size = "small", @data_toggle = "toggle", @data_onstyle = "success", @data_offstyle = "danger", @readonly = "true" , @disabled = "true"})
                    </label>
                </div>
            </div>
            <div class="form-group col-lg-2 col-md-6 col-xs-12">
                <label asp-for="CPRB" class="control-label"></label>
                <input asp-for="CPRB" class="form-control money" disabled/>
            </div>
        </div>


    </form>
</div>
