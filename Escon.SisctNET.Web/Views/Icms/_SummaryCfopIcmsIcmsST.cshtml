<table class="table" style="border:1px solid; font-size:11px;">
    <thead>
        <tr style="border:1px solid;">
            <th style="border:1px solid; text-align:center;" rowspan="2">
                UF
            </th>
            <th style="border:1px solid; text-align:center;" rowspan="2">
                CFOP
            </th>
            <th style="border:1px solid; text-align:center; font-size:8px;" rowspan="2">
                CST/CSOSN
            </th>
            <th style="border:1px solid; text-align:center;" rowspan="2">
                Valor Contábil
            </th>
            <th style="border:1px solid; text-align:center;" colspan="5">
                ICMS NORMAL
            </th>
            <th style="border:1px solid; text-align:center;" colspan="6">
                ICMS ST
            </th>
        </tr>
        <tr style="border:1px solid;">
            <th style="border:1px solid; text-align:center;">
                BC ICMS
            </th>
            <th style="border:1px solid; text-align:center;">
                ICMS (%)
            </th>
            <th style="border:1px solid; text-align:center;">
                Valor ICMS
            </th>
            <th style="border:1px solid; text-align:center;">
                FECOP (%)
            </th>
            <th style="border:1px solid; text-align:center;">
                Valor FECOP
            </th>
            <th style="border:1px solid; text-align:center;">
                BC ICMS
            </th>
            <th style="border:1px solid; text-align:center;">
                MVA (%)
            </th>
            <th style="border:1px solid; text-align:center;">
                ICMS (%)
            </th>
            <th style="border:1px solid; text-align:center;">
                Valor ICMS
            </th>
            <th style="border:1px solid; text-align:center;">
                FECOP (%)
            </th>
            <th style="border:1px solid; text-align:center;">
                Valor FECOP
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var cfop in ViewBag.Cfop)
        {
            <tr style="border:1px solid; font-size: 9px;">
                <td style="border:1px solid; padding-left:1%;">
                    @cfop[16]
                </td>
                <td style="border:1px solid; padding-left:1%;">
                    @cfop[0] - @cfop[1]
                </td>
                <td style="border:1px solid; padding-left:1%;">
                    @cfop[17]
                </td>
                <td style="border:1px solid; text-align:right; padding-right:1%;">
                    <b>@string.Format("{0:N}", Convert.ToDecimal(cfop[2].Replace(',', '*').Replace('.', ',').Replace('*', '.')))</b>
                </td>
                <td style="border:1px solid; text-align:right; padding-right:1%;">
                    <b>@string.Format("{0:N}", Convert.ToDecimal(cfop[3].Replace(',', '*').Replace('.', ',').Replace('*', '.')))</b>
                </td>
                <td style="border:1px solid; text-align:right; padding-right:1%;">
                    <b>@string.Format("{0:N}", Convert.ToDecimal(cfop[4].Replace(',', '*').Replace('.', ',').Replace('*', '.')))</b>
                </td>
                <td style="border:1px solid; text-align:right; padding-right:1%;">
                    <b>@string.Format("{0:N}", Convert.ToDecimal(cfop[5].Replace(',', '*').Replace('.', ',').Replace('*', '.')))</b>
                </td>
                <td style="border:1px solid; text-align:right; padding-right:1%;">
                    <b>@string.Format("{0:N}", Convert.ToDecimal(cfop[6].Replace(',', '*').Replace('.', ',').Replace('*', '.')))</b>
                </td>
                <td style="border:1px solid; text-align:right; padding-right:1%;">
                    <b>@string.Format("{0:N}", Convert.ToDecimal(cfop[7].Replace(',', '*').Replace('.', ',').Replace('*', '.')))</b>
                </td>
                <td style="border:1px solid; text-align:right; padding-right:1%;">
                    <b>@string.Format("{0:N}", Convert.ToDecimal(cfop[9].Replace(',', '*').Replace('.', ',').Replace('*', '.')))</b>
                </td>
                <td style="border:1px solid; text-align:right; padding-right:1%;">
                    <b>@string.Format("{0:N}", Convert.ToDecimal(cfop[15].Replace(',', '*').Replace('.', ',').Replace('*', '.')))</b>
                </td>
                <td style="border:1px solid; text-align:right; padding-right:1%;">
                    <b>@string.Format("{0:N}", Convert.ToDecimal(cfop[10].Replace(',', '*').Replace('.', ',').Replace('*', '.')))</b>
                </td>
                <td style="border:1px solid; text-align:right; padding-right:1%;">
                    <b>@string.Format("{0:N}", Convert.ToDecimal(cfop[11].Replace(',', '*').Replace('.', ',').Replace('*', '.')))</b>
                </td>
                <td style="border:1px solid; text-align:right; padding-right:1%;">
                    <b>@string.Format("{0:N}", Convert.ToDecimal(cfop[13].Replace(',', '*').Replace('.', ',').Replace('*', '.')))</b>
                </td>
                <td style="border:1px solid; text-align:right; padding-right:1%;">
                    <b>@string.Format("{0:N}", Convert.ToDecimal(cfop[14].Replace(',', '*').Replace('.', ',').Replace('*', '.')))</b>
                </td>
            </tr>
        }
    </tbody>
</table>