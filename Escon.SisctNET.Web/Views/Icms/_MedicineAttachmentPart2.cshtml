<div style="width:100%; padding-top:10px;">
    <table style="border:2px solid; font-size:12px;">
        <thead>
            <tr>
                <th style="text-align:center;border:1px solid;" width="1200px" colspan="8">
                    PARTE II - OPERAÇÕES COM AS DEMAIS MERCADORIAS NA FORMA DO ART. 783, INCISO II, DO RICMS
                </th>
            </tr>
            <tr>
                <th style="text-align:center;border:1px solid;" width="1200px" colspan="8">
                    A - Operações Internas
                </th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td style="text-align:center;border:1px solid;">
                    NF de Saída
                </td>
                <td style="text-align:center;border:1px solid;">
                    Destinatário
                </td>
                <td style="text-align:center;border:1px solid;">
                    UF
                </td>
                <td style="text-align:center;border:1px solid;">
                    CNPJ
                </td>
                <td style="text-align:center;border:1px solid;">
                    IE
                </td>
                <td style="text-align:center;border:1px solid;">
                    Valor da NF
                </td>
                <td style="text-align:center;border:1px solid;" width="100px">
                    % de Crédito Presumido
                </td>
                <td style="text-align:center;border:1px solid;">
                    Valor do Crédito
                </td>
            </tr>
            @for (int i = 0; i < ViewBag.DaselencadaInterna.Count; i++)
            {
                <tr>
                    <td style="text-align:center;border:1px solid;">
                        @ViewBag.DaselencadaInterna[i][0]
                    </td>
                    <td style="text-align:left;border:1px solid;padding-left:1%;">
                        @ViewBag.DaselencadaInterna[i][1]
                    </td>
                    <td style="text-align:center;border:1px solid;">
                        @ViewBag.DaselencadaInterna[i][2]
                    </td>
                    <td style="text-align:center;border:1px solid;">
                        @ViewBag.DaselencadaInterna[i][3]
                    </td>
                    <td style="text-align:center;border:1px solid;">
                        @ViewBag.DaselencadaInterna[i][4]
                    </td>
                    <td style="text-align:right;border:1px solid;padding-right:1%;">
                        @ViewBag.DaselencadaInterna[i][5]
                    </td>
                    <td style="text-align:right;border:1px solid;padding-right:1%;">
                        @ViewBag.DaselencadaInterna[i][7] %
                    </td>
                    <td style="text-align:right;border:1px solid;padding-right:1%;">
                        @ViewBag.DaselencadaInterna[i][6]
                    </td>
                </tr>
            }
            <tr>
                <td style="text-align:right;border:1px solid;padding-right:1%;" colspan="5">
                    TOTAL
                </td>
                <td style="text-align:right;border:1px solid;padding-right:1%;">@ViewBag.ValorDaselencadaInterna</td>
                <td style="text-align:right;border:1px solid;padding-right:1%;">@ViewBag.PercentualDaselencadaInterna %</td>
                <td style="text-align:right;border:1px solid;padding-right:1%;">@ViewBag.ValorCreditoDaselencadaInterna</td>
            </tr>
            <tr>
                <th style="text-align:center;border:1px solid;" width="1200px" colspan="8">
                    B - Operações Interestaduais
                </th>
            </tr>
            <tr>
                <td style="text-align:center;border:1px solid;">
                    NF de Saída
                </td>
                <td style="text-align:center;border:1px solid;">
                    Destinatário
                </td>
                <td style="text-align:center;border:1px solid;">
                    UF
                </td>
                <td style="text-align:center;border:1px solid;">
                    CNPJ
                </td>
                <td style="text-align:center;border:1px solid;">
                    IE
                </td>
                <td style="text-align:center;border:1px solid;">
                    Valor da NF
                </td>
                <td style="text-align:center;border:1px solid;">
                    % de Crédito Presumido
                </td>
                <td style="text-align:center;border:1px solid;">
                    Valor do Crédito
                </td>
            </tr>
            @for (int i = 0; i < ViewBag.DaselencadaInterestadual.Count; i++)
            {
                <tr>
                    <td style="text-align:center;border:1px solid;">
                        @ViewBag.DaselencadaInterestadual[i][0]
                    </td>
                    <td style="text-align:left;border:1px solid;padding-left:1%;">
                        @ViewBag.DaselencadaInterestadual[i][1]
                    </td>
                    <td style="text-align:center;border:1px solid;">
                        @ViewBag.DaselencadaInterestadual[i][2]
                    </td>
                    <td style="text-align:center;border:1px solid;">
                        @ViewBag.DaselencadaInterestadual[i][3]
                    </td>
                    <td style="text-align:center;border:1px solid;">
                        @ViewBag.DaselencadaInterestadual[i][4]
                    </td>
                    <td style="text-align:right;border:1px solid;padding-right:1%;">
                        @ViewBag.DaselencadaInterestadual[i][5]
                    </td>
                    <td style="text-align:right;border:1px solid;padding-right:1%;">
                        @ViewBag.DaselencadaInterestadual[i][7] %
                    </td>
                    <td style="text-align:right;border:1px solid;padding-right:1%;">
                        @ViewBag.DaselencadaInterestadual[i][6]
                    </td>
                </tr>
            }
            <tr>
                <td style="text-align:right;border:1px solid;padding-right:1%;" colspan="5">
                    TOTAL
                </td>
                <td style="text-align:right;border:1px solid;padding-right:1%;">@ViewBag.ValorDaselencadaInterestadual</td>
                <td style="text-align:right;border:1px solid;padding-right:1%;">@ViewBag.PercentualDaselencadaInterestadual %</td>
                <td style="text-align:right;border:1px solid;padding-right:1%;">@ViewBag.ValorCreditoDaselencadaInterestadual</td>
            </tr>
        </tbody>
    </table>
</div>
