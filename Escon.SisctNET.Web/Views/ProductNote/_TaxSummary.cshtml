
@if (ViewBag.Type == "Produto" || ViewBag.Type == "Nota" || ViewBag.Type == "NotaNI" || ViewBag.Type == "AgrupadoA" ||
   ViewBag.Type == "AgrupadoS" || ViewBag.Type == "ProdutoNI" || ViewBag.Type == "ProdutoI" || ViewBag.Type == "RegimeBA" || 
   ViewBag.Type == "RegimeBA2")
{
    @if (ViewBag.Type != "ProdutoI")
    {

        @await Html.PartialAsync("_TaxSummarySIE", new ViewDataDictionary(ViewData))

        @await Html.PartialAsync("_TaxSummaryIE", new ViewDataDictionary(ViewData))

    }
    else if (ViewBag.Incetive == true && ViewBag.Annex.Description.Equals("ANEXO ÚNICO"))
    {
        @await Html.PartialAsync("_TaxSummarySIE", new ViewDataDictionary(ViewData))

        @await Html.PartialAsync("_TaxSummaryIE", new ViewDataDictionary(ViewData))
    }

}

