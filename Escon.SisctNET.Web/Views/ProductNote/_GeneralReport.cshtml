@await Html.PartialAsync("_GeneralReportSIE", new ViewDataDictionary(ViewData))

@await Html.PartialAsync("_GeneralReportIE", new ViewDataDictionary(ViewData))

@if (ViewBag.Incetive == true && ViewBag.TypeIncetive == true)
{

    @await Html.PartialAsync("_GeneralReportIncentive", new ViewDataDictionary(ViewData))
    @await Html.PartialAsync("_GeneralTaxSummary", new ViewDataDictionary(ViewData))

}
else if (ViewBag.Incetive == true && ViewBag.TypeIncetive == false)
{
    @if (ViewBag.Comp.AnnexId != 3)
    {
        if (ViewBag.TypeCompany == true)
        {
            @await Html.PartialAsync("_GeneralProductIncentiveReport", new ViewDataDictionary(ViewData))
        }
        else
        {
            @await Html.PartialAsync("_GeneralIndustryIncentiveReport", new ViewDataDictionary(ViewData))
        }
    }
    else if (ViewBag.Comp.AnnexId == 3)
    {
        if (ViewBag.Comp.SectionId.Equals(2))
        {
            @await Html.PartialAsync("_GeneralReportIncentive", new ViewDataDictionary(ViewData))
            @await Html.PartialAsync("_GeneralTaxSummary", new ViewDataDictionary(ViewData))
        }
    }
}
else
{
    @await Html.PartialAsync("_GeneralTaxSummary", new ViewDataDictionary(ViewData))

}

