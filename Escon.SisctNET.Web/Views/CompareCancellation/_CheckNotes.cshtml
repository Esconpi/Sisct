<table class="table" style="width:100%;font-size:10px;">
    <thead>
        <tr style="border: 1px solid;">
            <th style="border: 1px solid;">
                NFe
            </th>
            <th style="border: 1px solid;">
                Modelo
            </th>
            <th style="border: 1px solid;">
                Chave
            </th>
        </tr>
    </thead>
    <tbody>
        @if (ViewBag.NotasCanceladas.Count > 0)
        {
            @for (int i = 0; i < ViewBag.NotasCanceladas[2].Count; i++)
            {
                <tr style="border: 1px solid;">
                    <td style="border: 1px solid;">@ViewBag.NotasCanceladas[2][i][2]</td>
                    <td style="border: 1px solid;">@ViewBag.NotasCanceladas[2][i][1]</td>
                    <td style="border: 1px solid;">@ViewBag.NotasCanceladas[2][i][0]</td>
                </tr>
            }
        }

        @if (ViewBag.Eventos.Count > 0)
        {
            @for (int i = 0; i < ViewBag.Eventos[2].Count; i++)
            {
                <tr style="border: 1px solid;">
                    <td style="border: 1px solid;">@ViewBag.Eventos[2][i][2]</td>
                    <td style="border: 1px solid;">@ViewBag.Eventos[2][i][1]</td>
                    <td style="border: 1px solid;">@ViewBag.Eventos[2][i][0]</td>
                </tr>
            }
        }


    </tbody>
</table>

<div class="col-lg-12">
    <h6>
        <strong>
            @if (ViewBag.NotasCanceladas.Count > 0 && ViewBag.Eventos.Count > 0)
            {
                @Convert.ToInt16(ViewBag.NotasCanceladas[2].Count + ViewBag.Eventos[2].Count) <span>registro(s)</span>
            }
            else
            {
                <span> 0 registro(s) </span>
            }

        </strong>
    </h6>
</div>
