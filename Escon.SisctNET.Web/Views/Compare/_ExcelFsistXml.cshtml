<table class="table" style="width:100%;font-size:10px;">
    <thead>
        <tr style="border: 1px solid;">
            <th style="border: 1px solid;">
                NFe
            </th>
            <th style="border: 1px solid;">
                Fornecedor
            </th>
            <th style="border: 1px solid;">
                Chave
            </th>
            <th style="border: 1px solid;">
                Valor
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var note in ViewBag.NotasFsist)
        {
            <tr style="border: 1px solid;">
                <td style="border: 1px solid;">@note[6]</td>
                <td style="border: 1px solid;">@note[9]</td>
                <td style="border: 1px solid;">@note[3]</td>
                <td style="border: 1px solid;">@string.Format("{0:N}", Convert.ToDecimal(note[11].Replace(',', '*').Replace('.', ',').Replace('*', '.')))</td>
            </tr>
        }
    </tbody>
</table>

<div class="col-lg-12">
    <h6>
        <strong>
            @ViewBag.NotasFsist.Count registro(s)
        </strong>
    </h6>
</div>
