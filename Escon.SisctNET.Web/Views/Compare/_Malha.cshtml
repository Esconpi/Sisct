<table class="table" style="width:100%;font-size:10px;">
    <thead>
        <tr style="border: 1px solid;">
            <th style="border: 1px solid;">
                NFe
            </th>
            <th style="border: 1px solid;">
                Data Emissão
            </th>
            <th style="border: 1px solid;">
                Total
            </th>
            <th style="border: 1px solid;">
                Chave
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var note in ViewBag.NotasExcel)
        {
            <tr style="border: 1px solid;">
                <td style="border: 1px solid;">@note[0]</td>
                <td style="border: 1px solid;">@note[1]</td>
                <td style="border: 1px solid;">@note[2]</td>
                <td style="border: 1px solid;">@note[5]</td>
            </tr>
        }
    </tbody>
</table>

<div class="col-lg-12">
    <h6>
        <strong>
            @ViewBag.NotasExcel.Count registro(s)
        </strong>
    </h6>
</div>
