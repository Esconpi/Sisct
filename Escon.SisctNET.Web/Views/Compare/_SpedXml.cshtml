<div><b>Notas Válidas</b></div>
<br />

<table class="table" style="width:100%;font-size:10px;">
    <thead>
        <tr style="border: 1px solid;">
            <th style="border: 1px solid;">Chave</th>
            <th style="border: 1px solid;">Modelo</th>
            <th style="border: 1px solid;">Número</th>
            <th style="border: 1px solid;">Valor</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var nota in ViewBag.notas_sped)
        {
            <tr>
                <th style="border: 1px solid;">@nota[0]</th>
                <th style="border: 1px solid;">@nota[1]</th>
                <th style="border: 1px solid;">@nota[2]</th>
                <th style="border: 1px solid;">@nota[3]</th>
            </tr>
        }
    </tbody>
</table>

<div class="col-lg-12">
    <h6>
        <strong>
            @ViewBag.notas_sped.Count registro(s)
        </strong>
    </h6>
</div>

@await Html.PartialAsync("_NotaInvalida", new ViewDataDictionary(ViewData))