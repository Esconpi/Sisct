<table class="table table-striped table-bordered" style="width:100%;font-size:10px;">
    <thead>
        <tr>
            <th>Chave</th>
            <th>Modelo</th>
            <th>Número</th>
            <th>Valor</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var nota in ViewBag.notas_sped)
        {
            <tr>
                <th>@nota[0]</th>
                <th>@nota[1]</th>
                <th>@nota[2]</th>
                <th>@string.Format("{0:N}", Convert.ToDecimal(nota[3]))</th>
            </tr>
        }
    </tbody>
</table>