<div class="col-lg-6">
    <b>Notas Válidas</b>

</div>
<div class="col-lg-6" style="text-align:right">
    <b>Quantidade:</b> @ViewBag.QtdValida
</div>
<br />

<table class="table" style="width:100%;font-size:10px;">
    <thead>
        <tr style="border: 1px solid;">
            <th style="border: 1px solid;">Chave</th>
            <th style="border: 1px solid;">Modelo</th>
            <th style="border: 1px solid;">Número</th>
            <th style="border: 1px solid;">Valor</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var nota in ViewBag.NotasSped)
        {
            <tr>
                <td style="border: 1px solid;">@nota[0]</td>
                <td style="border: 1px solid;">@nota[1]</td>
                <td style="border: 1px solid;">@nota[2]</td>
                <td style="border: 1px solid;">@nota[3]</td>
            </tr>
        }
    </tbody>
</table>

<div class="col-lg-12">
    <h6>
        <strong>
            @ViewBag.NotasSped.Count registro(s)
        </strong>
    </h6>
</div>

@await Html.PartialAsync("_NotaInvalida", new ViewDataDictionary(ViewData))